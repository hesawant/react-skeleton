{
    "extends": ["tslint:recommended"],
    "rules": {
        "arrow-parens": [true, "ban-single-arg-parens"],
        "indent": [true, "spaces"],
        "interface-name": [true, "never-prefix"],
        "no-unused-expression": [true, "allow-fast-null-checks"],
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "variable-name": false,
        "no-console": false,
        "member-access": false
    }
}
